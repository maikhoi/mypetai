{
  "name": "mypetai-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:chat": "tsc -p server/tsconfig.server.json",
    "build:server": "tsc -p server/tsconfig.server.json",
    "start:server": "node dist/server/index.js",
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:chat\"",
    "dev:next": "next dev",
    "dev:chat": "nodemon --watch server --ext ts --exec ts-node --project server/tsconfig.server.json server/index.ts",
    "build": "npm rebuild lightningcss && next build",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@next-auth/mongodb-adapter": "^1.1.3",
