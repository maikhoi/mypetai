{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "outDir": "../dist",
      "rootDir": "./",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "strict": false,
      "skipLibCheck": true,
      "types": ["node"],
      "noEmitOnError":false
    },
    "include": ["./**/*.ts"]
  }
  